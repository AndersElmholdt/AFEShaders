global proc AEinvertForceTemplate(string $nodeName)
{
    editorTemplate -beginScrollLayout;
    
    editorTemplate -beginLayout "Invert Force Attributes" -collapse 0;
        editorTemplate -addControl "inputArray";
		editorTemplate -addControl "outputArray";
    editorTemplate -endLayout;
    
    editorTemplate -addExtraControls;
    editorTemplate -endScrollLayout;
}